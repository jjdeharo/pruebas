{
  "categorias": [
    {
      "nombre": "Fracciones",
      "id": "fracciones",
      "grid_template_columns": "repeat(auto-fit, minmax(180px, 1fr))",
      "isCollapsed": false,
      "elementos": [
        {
          "type": "button",
          "latex": "\\frac{}{}",
          "display": "\\frac{3}{4}",
          "title": "Fracción"
        }
      ]
    },
    {
      "nombre": "Raíces y potencias",
      "id": "raices_potencias",
      "grid_template_columns": "repeat(auto-fit, minmax(180px, 1fr))",
      "isCollapsed": false,
      "elementos": [
        {
          "type": "button",
          "latex": "\\sqrt{}",
          "display": "\\sqrt{16}",
          "title": "Raíz cuadrada"
        }
      ]
    },
    {
      "nombre": "Ecuaciones",
      "id": "ecuaciones",
      "grid_template_columns": "repeat(auto-fit, minmax(180px, 1fr))",
      "isCollapsed": false,
      "elementos": [
        {
          "type": "button",
          "latex": "x + b = c",
          "display": "x + 3 = 7",
          "title": "Ecuación lineal"
        }
      ]
    },
    {
      "nombre": "Sistemas",
      "id": "sistemas",
      "grid_template_columns": "repeat(auto-fit, minmax(220px, 1fr))",
      "isCollapsed": false,
      "elementos": [
        {
          "type": "button",
          "latex": "\\begin{cases} \\\\ \\end{cases}",
          "display": "\\begin{cases} x + y = 5 \\\\ x - y = 1 \\end{cases}",
          "title": "Sistema de ecuaciones"
        }
      ]
    },
    {
      "nombre": "Geometría",
      "id": "geometria",
      "grid_template_columns": "repeat(auto-fit, minmax(180px, 1fr))",
      "isCollapsed": false,
      "elementos": [
        {
          "type": "button",
          "latex": "A = \\pi r^2",
          "display": "A = \\pi r^2",
          "title": "Área del círculo"
        }
      ]
    },
    {
      "nombre": "Cosas",
      "id": "cosas",
      "grid_template_columns": "repeat(auto-fit, minmax(180px, 1fr))",
      "isCollapsed": false,
      "elementos": [
        {
          "type": "button",
          "latex": "\\frac{}{}",
          "display": "\\frac{a}{b}",
          "title": "Fracción"
        },
        {
          "type": "button",
          "latex": "\\sqrt{}",
          "display": "\\sqrt{2}",
          "title": "Raíz cuadrada"
        },
        {
          "type": "button",
          "latex": "\\sum_{}^{}",
          "display": "\\sum_{i=1}^{n}",
          "title": "Sumatorio"
        },
        {
          "type": "button",
          "latex": "\\int_{}^{}",
          "display": "\\int_{a}^{b}",
          "title": "Integral definida"
        },
        {
          "type": "button",
          "latex": "\\left(\\right)",
          "display": "\\left( x+y \\right)",
          "title": "Paréntesis ajustables"
        },
        {
          "type": "button",
          "latex": "\\frac{d}{dx}",
          "display": "\\frac{d}{dx}f(x)",
          "title": "Derivada"
        },
        {
          "type": "button",
          "latex": "\\frac{\\partial}{\\partial x}",
          "display": "\\frac{\\partial}{\\partial x}f(x,y)",
          "title": "Derivada parcial"
        },
        {
          "type": "button",
          "latex": "E=mc^2",
          "display": "E=mc^2",
          "title": "Energía-masa"
        },
        {
          "type": "button",
          "latex": "F=ma",
          "display": "F=ma",
          "title": "Segunda ley de Newton"
        },
        {
          "type": "button",
          "latex": "p=mv",
          "display": "p=mv",
          "title": "Cantidad de movimiento"
        },
        {
          "type": "button",
          "latex": "V=IR",
          "display": "V=IR",
          "title": "Ley de Ohm"
        }
      ]
    }
  ]
}