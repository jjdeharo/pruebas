{
  "categorias": [
    {
      "nombre": "Fracciones",
      "id": "fracciones",
      "grid_template_columns": "repeat(auto-fit, minmax(180px, 1fr))",
      "isCollapsed": false,
      "elementos": [
        {
          "type": "button",
          "latex": "\\frac{}{}",
          "display": "\\frac{3}{4}",
          "title": "Fracción"
        }
      ]
    },
    {
      "nombre": "Raíces y potencias",
      "id": "raices_potencias",
      "grid_template_columns": "repeat(auto-fit, minmax(180px, 1fr))",
      "isCollapsed": false,
      "elementos": [
        {
          "type": "button",
          "latex": "\\sqrt{}",
          "display": "\\sqrt{16}",
          "title": "Raíz cuadrada"
        }
      ]
    },
    {
      "nombre": "Ecuaciones",
      "id": "ecuaciones",
      "grid_template_columns": "repeat(auto-fit, minmax(180px, 1fr))",
      "isCollapsed": false,
      "elementos": [
        {
          "type": "button",
          "latex": "x + b = c",
          "display": "x + 3 = 7",
          "title": "Ecuación lineal"
        }
      ]
    },
    {
      "nombre": "Sistemas",
      "id": "sistemas",
      "grid_template_columns": "repeat(auto-fit, minmax(220px, 1fr))",
      "isCollapsed": false,
      "elementos": [
        {
          "type": "button",
          "latex": "\\begin{cases} \\\\ \\end{cases}",
          "display": "\\begin{cases} x + y = 5 \\\\ x - y = 1 \\end{cases}",
          "title": "Sistema de ecuaciones"
        }
      ]
    },
    {
      "nombre": "Geometría",
      "id": "geometria",
      "grid_template_columns": "repeat(auto-fit, minmax(180px, 1fr))",
      "isCollapsed": false,
      "elementos": [
        {
          "type": "button",
          "latex": "A = \\pi r^2",
          "display": "A = \\pi r^2",
          "title": "Área del círculo"
        }
      ]
    }
  ]
}